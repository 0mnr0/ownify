{
  "manifest_version": 3,
  "name": "SOCKS5 Proxy",
  "version": "1.0",
  "permissions": [
    "privacy",
	"scripting",
    "proxy",
    "storage",
	"webRequest",
	"webRequestAuthProvider",
	"declarativeNetRequest",
	"declarativeNetRequestWithHostAccess",
	"tabs",
	"<all_urls>"
  ],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup/popup.html"
  },
  "icons": {
    "128": "icon.png"
  },
  "minimum_chrome_version": "110",
  "incognito": "split"
}